def frequency_of_numbers(arr):
    freq_dict = {}

    for num in arr:
        if num in freq_dict:
            freq_dict[num] += 1
        else:
            freq_dict[num] = 1

    sorted_freq_dict = sorted(freq_dict.items())

    for key, value in sorted_freq_dict:
        print(key, value)

arr = list(map(int, input().split()))

frequency_of_numbers(arr)
